<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/homeFragment2">

    <fragment
        android:id="@+id/homeFragment2"
        android:name="com.example.onmyway.homeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment2_to_setUpFragment"
            app:destination="@id/setUpFragment" />
    </fragment>
    <fragment
        android:id="@+id/setUpFragment"
        android:name="com.example.onmyway.setUpFragment"
        android:label="fragment_set_up"
        tools:layout="@layout/fragment_set_up" >
        <action
            android:id="@+id/action_setUpFragment_to_runningFragment2"
            app:destination="@id/runningFragment2" />
    </fragment>
    <fragment
        android:id="@+id/runningFragment2"
        android:name="com.example.onmyway.runningFragment"
        android:label="fragment_running"
        tools:layout="@layout/fragment_running" />
    <fragment
        android:id="@+id/savedFragment2"
        android:name="com.example.onmyway.savedFragment"
        android:label="fragment_saved"
        tools:layout="@layout/fragment_saved" >
        <action
            android:id="@+id/action_savedFragment2_to_setUpFragment"
            app:destination="@id/setUpFragment" />
        <action
            android:id="@+id/action_savedFragment2_to_runningFragment2"
            app:destination="@id/runningFragment2" />
    </fragment>
</navigation>